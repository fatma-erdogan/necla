pipeline {
    agent any
    environment {
        FATMA="NECLAAAAAAAMMM"
    }
    stages {
        stage('Hello') {
            steps {
                echo "canim hosgeldin ${FATMA}"
                sh 'echo second step'
            }
        }
        stage('iki') {
            steps {
                echo 'Hello World canim nasilsin iyi misin '
            }
        }
        stage('uc') {
            steps {
                echo 'Hello World canim nasilsin iyi misin '
            }
        }
    }
    
}
